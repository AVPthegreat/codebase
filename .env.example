# Copy this file to .env and customize values before using docker compose

# Postgres
POSTGRES_DB=onlinejudge
POSTGRES_USER=onlinejudge
POSTGRES_PASSWORD=onlinejudge
POSTGRES_HOST_PORT=5435

# Redis  
REDIS_HOST=oj-redis
REDIS_PORT=6379
REDIS_HOST_PORT=6380

# Environment
OJ_ENV=production

# Judge server token (change in production!)
JUDGE_SERVER_TOKEN=token123

# Judge server URLs (use container names for networking)
JUDGE_SERVER_BACKEND_URL=http://oj-app:8000/api/judge_server_heartbeat/
JUDGE_SERVER_SERVICE_URL=http://oj-judge-server:8080
